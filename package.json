    "buble": "0.19.4",
    "docz": "^0.12.13",
    "docz-plugin-github-page": "^1.0.4",
    "node-polyglot": "^2.3.0",
    "normalize.scss": "^0.1.0",
    "prop-types": "^15.6.2",
    "react-grid-system": "^4.1.4",
    "styled-components": "^4.0.2",
    "uuid": "^3.3.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "homepage": "https://nuvemtecnologia.github.io/nimbos/",
  "jest": {
    "setupFiles": [
      "./test/jest-setup.js"

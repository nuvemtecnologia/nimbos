    "regenerator-runtime": "^0.12.0",
    "sinon": "^6.1.4"
  },
  "scripts": {
    "build": "rm -rf docs && docz build && cp .docz/404.html docs/ && cp .docz/redirect.js docs/",
    "docz:dev": "docz dev",
    "start": "docz dev",
    "test": "jest",
    "bootstrap": "npm i -g verdaccio",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
